{
	"reg" : {
		"error" : "[0-9]+ +[0-9]+",
		"ops" : "([-+*/()]|ABS|SQRT)", 
		"space_head" : "^ +",
		"space_tail" : " +$"	
	},
	"op" : {
		"/" : {
			"params" : 2,
			"priority" : 20,
			"code" : "p2 / p1"
		},
		"*" : {
			"params" : 2,
			"priority" : 20,
			"code" : "p2 * p1"
		},
		"+" : {
			"params" : 2,
			"priority" : 10,
			"code" : "p2 + p1"
		},
		"-" : {
			"params" : 2,
			"priority" : 10,
			"code" : "p2 - p1"
		},
		"ABS" : {
			"params" : 1,
			"priority" : 30,
			"code" : "(p1 < 0) ? -p1 : p1"
		},
		"SQRT" : {
			"params" : 1,
			"priority" : 30,
			"code" : "Math.sqrt(p1)"
		}
	}
}
